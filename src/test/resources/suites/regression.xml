<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Test" parallel="tests" thread-count="3">
    <test name="Test1">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="homework7.tests.CheckoutCompleteTests"/>
            <class name="homework7.tests.ProductDetailsTests"/>
        </classes>
    </test>
    <test name="Test2">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="homework7.tests.CheckOverviewTests"/>
        </classes>
    </test>
</suite>
